{
  "name": "nurospace",
  "private": true,
  "workspaces": [
    "apps/*",
    "apps/web/*",
    "packages/*"
  ],
  "scripts": {
    "check": "biome check --write .",
    "format": "biome format --write .",
    "prepare": "husky",
    "dev": "turbo dev",
    "build": "turbo build",
    "check-types": "turbo check-types",
    "dev:www": "turbo -F @apps/www dev",
    "dev:api": "turbo -F @apps/api dev",
    "dev:dashboard": "turbo run dev --filter=@apps/api --filter=@apps/dashboard"
  },
  "devDependencies": {
    "turbo": "^2.5.5",
    "@biomejs/biome": "^2.1.3",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2"
  },
  "lint-staged": {
    "*.{js,ts,cjs,mjs,d.cts,d.mts,jsx,tsx,json,jsonc}": [
      "biome format --write . && biome check ."
    ]
  },
  "packageManager": "bun@1.2.17",
  "dependencies": {
    "@langchain/core": "^0.3.68",
    "@langchain/google-genai": "^0.2.16",
    "dotenv": "^17.2.1",
    "nurospace": "."
  }
}
